    "@tanstack/react-virtual": "^3.0.0-beta.23",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/sanitize-html": "^2.8.0",
    "@zoralabs/nft-hooks": "^1.2.1",
    "@types/react-window-infinite-loader": "^1.0.6",
    "connectkit": "^1.1.2",
    "date-fns": "^2.29.2",
    "ethers": "^5.7.2",
    "framer-motion": "^7.2.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-hot-toast": "^2.3.0",
    "react-infinite-scroller": "^1.2.6",
    "react-intersection-observer": "^9.4.2",
    "react-markdown": "^8.0.3",
    "react-relay": "^14.1.0",
    "react-responsive": "^9.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-textfit": "^1.1.1",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.8",
    "react-window-scroller": "^1.0.10",
    "recoil": "^0.7.5",
    "relay-runtime": "^14.1.0",
    "remark-breaks": "^3.0.2",
    "rehype-raw": "^6.1.1",
    "sanitize-html": "^2.9.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.4.2",
    "wagmi": "^0.10.15",
    "web-vitals": "^2.1.0"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript",
    "customScalars": {
      "BigInt": "string",
      "Timestamp": "number"
    }
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn run codegen && react-scripts start",
    "build": "yarn run codegen && react-scripts build",
    "relay": "yarn run relay-compiler",
    "check": "yarn run tsc --noEmit",
    "generate-typechain": "typechain --target ethers-v5 --out-dir src/contracts/generated src/contracts/abis/*.json",
    "codegen": "yarn run relay && yarn run generate-typechain",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "relay"
    ],
    "rules": {
      "relay/generated-flow-types": "off"
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:relay/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/lodash": "^4.14.184",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-relay": "^14.1.1",
    "@types/react-textfit": "^1.1.0",
    "@types/react-window": "^1.8.5",
    "@types/relay-runtime": "^14.1.2",
    "autoprefixer": "^10.4.8",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-relay": "^1.8.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.1.0",
    "typechain": "^8.1.0"
  }
}
