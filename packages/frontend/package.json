{
  "name": "@agora/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@agora/common": "workspace:*",
    "@emotion/css": "^11.10.0",
    "@graphql-tools/schema": "^9.0.2",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.10",
    "@nouns/assets": "^0.4.2",
    "@nouns/sdk": "^0.4.0",
    "@restart/ui": "^1.3.1",
    "@sentry/react": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-virtual": "^3.0.0-beta.23",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-window-infinite-loader": "^1.0.6",
    "@types/sanitize-html": "^2.8.0",
    "@zoralabs/nft-hooks": "^1.2.1",
    "abitype": "^0.7.1",
    "connectkit": "^1.1.2",
    "date-fns": "^2.29.2",
    "ethers": "5.7.0",
    "framer-motion": "^6.5.1",
    "lodash": "^4.17.21",
    "openai": "^3.1.0",
    "openai-streams": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-hot-toast": "^2.3.0",
    "react-infinite-scroller": "^1.2.6",
    "react-intersection-observer": "^9.4.2",
    "react-markdown": "^8.0.3",
    "react-relay": "^14.1.0",
    "react-responsive": "^9.0.0",
    "react-router-dom": "^6.3.0",
    "react-textfit": "^1.1.1",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.8",
    "react-window-scroller": "^1.0.10",
    "recoil": "^0.7.5",
    "rehype-raw": "^6.1.1",
    "relay-runtime": "^14.1.0",
    "remark-breaks": "^3.0.2",
    "sanitize-html": "^2.9.0",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.9.5",
    "wagmi": "^0.10.15",
    "web-vitals": "^2.1.0",
    "yield-stream": "^2.3.0",
    "zod": "3.14.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "@types/lodash": "^4.14.184",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-relay": "^14.1.1",
    "@types/react-textfit": "^1.1.0",
    "@types/react-window": "^1.8.5",
    "@types/relay-runtime": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-relay": "^13.0.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.34.0",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-relay": "^1.8.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "vite": "^4.2.1",
    "vite-plugin-relay": "^2.0.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vitest": "^0.29.8"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "concurrently --kill-others 'vite' 'yarn relay --watch'",
    "lint": "eslint --fix '**/*.{ts,tsx}'",
    "build": "yarn run codegen && vite build",
    "relay": "yarn run relay-compiler",
    "check": "yarn run tsc --noEmit",
    "codegen": "yarn run relay",
    "test": "vitest"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "customScalars": {
      "BigInt": "string",
      "Timestamp": "number"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "relay",
      "ban",
      "import"
    ],
    "rules": {
      "relay/generated-flow-types": "off",
      "ban/ban": [
        "error",
        {
          "name": "parseInt",
          "message": "Use BigNumber.from(value).toNumber() instead, it throws when provided invalid values instead of coercing"
        }
      ],
      "import/no-duplicates": "error",
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*Route.ts"
        ],
        "rules": {
          "relay/unused-fields": "off",
          "relay/must-colocate-fragment-spreads": "off"
        }
      }
    ],
    "extends": [
      "plugin:relay/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
