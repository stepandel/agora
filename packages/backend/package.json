{
  "private": true,
  "version": "0.1.0",
  "name": "nouns-agora-backend",
  "scripts": {
    "generate-schema": "ts-node --require ./scripts/registerGraphQL.ts src/bin/scripts/generateSchema.ts > ../frontend/schema.graphql",
    "dump-namespace": "ts-node --transpileOnly src/bin/scripts/dumpNamespace.ts",
    "loadStatements": "ts-node --transpileOnly src/bin/scripts/loadStatementsIntoDynamo.ts",
    "check": "tsc --noEmit",
    "fetch": "ts-node src/indexer/bin/fetch.ts",
    "backfill": "ts-node src/indexer/bin/backfill.ts",
    "tail": "ts-node src/indexer/bin/tail.ts",
    "dump": "ts-node src/indexer/bin/dump.ts",
    "load": "ts-node src/indexer/bin/load.ts",
    "load-remote": "ts-node --transpileOnly src/indexer/ops/bin/load.ts",
    "send-admin-message": "ts-node --transpileOnly src/indexer/ops/bin/sendAdminMessage.ts",
    "server": "ts-node --require ./scripts/registerGraphQL.ts src/bin/server.ts",
    "generate-typechain": "typechain --target ethers-v5 --out-dir src/contracts/generated src/contracts/abis/*.json",
    "generate-schema-types": "graphql-codegen",
    "generate": "yarn run generate-typechain && yarn run generate-schema-types",
    "build-worker": "node scripts/buildWorker.js",
    "visualize-worker-bundle": "esbuild-visualizer ---metadata ./dist/meta.json",
    "stream": "ts-node src/indexer/ops/bin/stream.ts",
    "start": "yarn run generate && yarn run server",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.199.0",
    "@aws-sdk/client-s3": "^3.210.0",
    "@aws/dynamodb-auto-marshaller": "^0.7.1",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@envelop/core": "^2.6.0",
    "@envelop/response-cache": "^3.2.0",
    "@fast-check/jest": "^1.6.0",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@graphql-tools/batch-execute": "^8.5.5",
    "@graphql-tools/delegate": "^9.0.4",
    "@graphql-tools/merge": "^8.3.4",
    "@graphql-tools/stitch": "^8.7.7",
    "@graphql-yoga/node": "^2.13.12",
    "@sentry/integrations": "7.28.1",
    "aws-sdk": "^2.1254.0",
    "dataloader": "^2.1.0",
    "alchemy-sdk": "^2.2.5",
    "date-fns": "^2.29.3",
    "discord-api-types": "^0.37.15",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.6.0",
    "heap": "^0.2.7",
    "limiter": "^2.1.0",
    "level": "^8.0.0",
    "lmdb": "^2.7.7",
    "marked": "^4.1.0",
    "node-fetch": "^3.2.10",
    "progress": "^2.0.3",
    "toucan-js": "^3.1.0",
    "uuid": "^9.0.0",
    "web3": "^1.8.1",
    "ws": "^8.12.0",
    "wrangler": "^2.9.0",
    "zod": "3.14.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230214.0",
    "@envelop/apollo-tracing": "^3.6.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/client-preset": "^1.1.3",
    "@graphql-yoga/common": "^2.12.11",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29.0.1",
    "@types/marked": "^4.0.6",
    "@types/heap": "^0.2.31",
    "@types/node": "^18.7.18",
    "@types/progress": "^2.0.5",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.4",
    "dedent": "^0.7.0",
    "esbuild": "^0.15.7",
    "esbuild-plugin-copy": "^1.3.0",
    "esbuild-plugin-wasm-pack": "^1.1.0",
    "esbuild-visualizer": "^0.3.1",
    "fast-check": "^3.6.0",
    "graphql-request": "^5.0.0",
    "jest": "^29.0.3",
    "jest-junit": "^14.0.1",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4"
  }
}
